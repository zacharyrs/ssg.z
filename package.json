{
  "name": "static.z",
  "description": "my custom design static site generator (because there aren't enough already...)",
  "version": "0.0.1",
  "author": {
    "name": "zacharyrs",
    "email": "git@zacharyrs.me"
  },
  "repository": "git@github.com:zacharyrs/static.z.git",
  "main": "scripts/main/index.js",
  "dependencies": {},
  "sideEffects": [
    "*.css",
    "*.html"
  ],
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "babel-loader": "8.0.6",
    "babel-preset-minify": "0.5.0",
    "browser-sync": "2.26.5",
    "clean-webpack-plugin": "2.0.2",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "cz-customizable": "6.2.0",
    "doiuse": "4.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "3.1.0",
    "extract-css-chunks-webpack-plugin": "4.3.2",
    "file-loader": "3.0.1",
    "generate-robotstxt": "6.0.1",
    "glob": "7.1.4",
    "gray-matter": "4.0.2",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-data": "1.3.1",
    "gulp-favicons": "2.3.0",
    "gulp-footer": "2.0.2",
    "gulp-if": "2.0.2",
    "gulp-jsbeautifier": "3.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-pug": "4.0.1",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.2",
    "highlight.js": "9.15.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "loader-utils": "1.2.3",
    "markdown-it": "8.4.2",
    "markdown-it-anchor": "5.0.2",
    "markdown-it-attrs": "2.3.4",
    "markdown-it-container": "2.0.0",
    "markdown-it-fontawesome": "0.3.0",
    "markdown-it-footnote": "3.0.1",
    "markdown-it-header-sections": "1.0.0",
    "markdown-it-multimd-table": "3.1.3",
    "markdown-it-sub": "1.0.0",
    "markdown-it-sup": "1.0.0",
    "node-sass": "",
    "node-sass-tilde-importer": "1.0.2",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss": "7.0.16",
    "postcss-easing-gradients": "3.0.1",
    "postcss-easings": "2.0.0",
    "postcss-font-magician": "2.2.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "postcss-reporter": "6.0.1",
    "postcss-short": "5.0.0",
    "postcss-sorting": "5.0.0",
    "prettier": "1.17.1",
    "prettier-loader": "2.1.1",
    "pug": "2.0.3",
    "responsive-loader": "1.2.0",
    "sass-loader": "7.1.0",
    "schema-utils": "1.0.0",
    "sharp": "",
    "sqip": "0.3.3",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.1.0",
    "stylelint-order": "3.0.0",
    "stylelint-scss": "3.6.1",
    "stylemark": "3.1.3",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-middleware": "3.7.0",
    "webpack-fix-style-only-entries": "0.2.1",
    "webpack-hot-middleware": "2.25.0",
    "webpackbar": "3.2.0",
    "xo": "0.24.0"
  },
  "browserslist": [
    "ie >= 10",
    "> 1%",
    "not OperaMini all"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,md,scss}": [
        "prettier",
        "xo",
        "git add"
      ]
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development gulp dev --cwd ./src/ --gulpfile ./scripts/gulp",
    "style": "NODE_ENV=development gulp styleguide --cwd ./src/ --gulpfile ./scripts/gulp",
    "build": "NODE_ENV=production gulp prod --cwd ./src/ --gulpfile ./scripts/gulp"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "space": true,
    "semicolon": false,
    "prettier": true
  }
}
